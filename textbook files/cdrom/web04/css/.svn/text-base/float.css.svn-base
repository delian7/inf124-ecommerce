/* float.css */

body {
font-size: 1.5em;
}

div#page {
width: 500px;
background-color: silver;
}

div#header {
width: 100%;
background-color: red;
}

div#menu {
float: left;
width: 35%;
background-color: lime;
}

div#content {
float: left;
width: 65%;
}

div#footer {
clear: left;
width: 100%;
background-color: teal;
}
